<div class="row">
  <div class="col-md-10 principal">
    <div class="row mt-3 principal">
      <div class="col-md-4 videocss">
        <div id="video-grid"></div>
      </div>
      <div class="col-md-8 screenstyle">
        <div class="row row__videoscreen">
          <video id="videoscreen" autoplay></video>
        </div>
        <div class="row menubtn">
          <div class="contenedor__btn">
            <div class="col-md-3 btn__menuone">
              <div class="btn__accion" id="muteButton" onclick="muteUnmute()">
                <i class="fa fa-microphone btn_ico"></i>
              </div>
              <div class="" id="playPauseVideo" onclick="playStop()">
                <i class="fa fa-video-camera btn_ico"></i>
              </div>
              <div class="" id="muteButton" onclick="">
                <i class="fa fa-volume-up btn_ico"></i>
              </div>
            </div>
            <div class="col-md-6 btn__menutwo">
              <div class="" id="start">
                <i class="fa fa-expand btn_ico"></i>
              </div>
            </div>
            <div class="col-md-3 btn__menuthree">
              <div class="btn_ico_fin" onclick="redireccionar()">
                Finalizar llamada
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-md-2 bg-chat">
    <div class="row" id="global">
      <div class="col-md-12 messages">
        <div class="main__chat__window" id="main__chat__window">
          <ul class="" id="all_messages"></ul>
        </div>
      </div>
    </div>
    <div class="row main__message_container">
      <input
        type="text"
        id="chat_message"
        autofocus
        placeholder="¡Escribe algo aquí!"
      />
    </div>
  </div>
  <div class="chatnuevo">
    <a href="#btn_botton" id="down"><i class="fa fa-chevron-circle-down"></i></a>
  </div>
  <hr class="btnbotton" id="btn_botton"></hr>
</div>
